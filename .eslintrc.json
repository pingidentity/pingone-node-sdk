{
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2015,
    "ecmaFeatures": {
      "jsx": true,
      "classes": true
    },
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true,
    "jasmine": true
  },
  "plugins": ["import"],
  "extends": ["eslint:recommended", "plugin:import/recommended"],
  "settings": {
    "import/ignore": ["node_modules"],
    "polyfills": ["Promise", "Array.from", "TextEncoder"]
  },
  "rules": {
    "array-bracket-spacing": 2,
    "arrow-parens": [2, "as-needed"],
    "arrow-spacing": 2,
    "block-spacing": 2,
    "camelcase": [
      2,
      {
        "properties": "always"
      }
    ],
    "comma-dangle": 2,
    "comma-style": 2,
    "computed-property-spacing": 2,
    "curly": [2],
    "dot-location": [2, "property"],
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": [2],
    "func-call-spacing": 2,
    "generator-star-spacing": [2, "after"],
    "import/first": 2,
    "import/newline-after-import": 2,
    "import/no-absolute-path": 2,
    "import/no-deprecated": 2,
    "import/no-extraneous-dependencies": 2,
    "import/unambiguous": [0],
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1
      }
    ],
    "jsx-quotes": [2, "prefer-double"],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "keyword-spacing": [
      2,
      {
        "before": true,
        "after": true
      }
    ],
    "linebreak-style": 2,
    "max-depth": [2, 5],
    "max-len": [
      2,
      120,
      4,
      {
        "ignoreComments": true,
        "ignoreUrls": true
      }
    ],
    "max-params": [2, 6],
    "new-cap": [
      2,
      {
        "newIsCap": true,
        "capIsNew": false
      }
    ],
    "new-parens": [2],
    "no-alert": 2,
    "no-caller": 2,
    "no-console": 1,
    "no-debugger": 2,
    "no-empty-function": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-implicit-coercion": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-mixed-operators": 2,
    "no-multiple-empty-lines": 0,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-param-reassign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-tabs": 2,
    "no-template-curly-in-string": 2,
    "no-throw-literal": 2,
    "no-unmodified-loop-condition": 2,
    "no-unsafe-negation": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-useless-constructor": 2,
    "no-useless-escape": 2,
    "no-useless-rename": 2,
    "no-void": 2,
    "no-whitespace-before-property": 2,
    "no-cond-assign": [2],
    "no-delete-var": [2],
    "no-dupe-args": [2],
    "no-dupe-keys": [2],
    "no-duplicate-case": [2],
    "no-extra-semi": [2],
    "no-func-assign": [2],
    "no-invalid-regexp": [2],
    "no-irregular-whitespace": [2],
    "no-loop-func": [2],
    "no-mixed-spaces-and-tabs": [2],
    "no-multi-spaces": [
      2,
      {
        "exceptions": {
          "Property": false
        }
      }
    ],
    "no-plusplus": [2],
    "no-shadow-restricted-names": [2],
    "no-shadow": [2],
    "no-trailing-spaces": [
      2,
      {
        "skipBlankLines": true
      }
    ],
    "no-undef": [2],
    "no-undef-init": [2],
    "no-underscore-dangle": [0],
    "no-unexpected-multiline": [2],
    "no-unreachable": [2],
    "no-unused-expressions": [2],
    "no-unused-vars": [2],
    "no-use-before-define": [2, "nofunc"],
    "object-curly-newline": [
      0,
      {
        "multiline": true
      }
    ],
    "object-shorthand": 0,
    "one-var": [0, "never"],
    "operator-linebreak": [
      2,
      "after",
      {
        "overrides": {
          "?": "before",
          ":": "before"
        }
      }
    ],
    "padded-blocks": [2, "never"],
    "prefer-arrow-callback": 0,
    "prefer-const": 2,
    "prefer-numeric-literals": 2,
    "prefer-reflect": 2,
    "prefer-rest-params": 2,
    "prefer-spread": 2,
    "prefer-template": 2,
    "quote-props": [0, "as-needed"],
    "quotes": [2, "double", "avoid-escape"],
    "radix": [2, "always"],
    "rest-spread-spacing": 2,
    "semi": [2, "always"],
    "semi-spacing": 2,
    "space-before-blocks": [2],
    "space-before-function-paren": [2],
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": 2,
    "symbol-description": 2,
    "strict": [0],
    "template-curly-spacing": 2,
    "valid-jsdoc": [
      1,
      {
        "requireReturn": false
      }
    ],
    "valid-typeof": [2],
    "yield-star-spacing": 2
  }
}
